all.export /
#xrd.protocol xrootd *
#xrd.port 1094
#all.adminpath /var/log/xrootd
#all.pidpath /var/run/xrootd
#xrootd.export /
oss.localroot /tmp/
xrootd.seclib /usr/lib/alice-xrootd/libXrdSec.so
sec.protocol /usr/lib/alice-xrootd unix
ofs.authlib /usr/lib/alice-xrootd/libXrdAliceTokenAcc.so
ofs.authorize
#xrootd.fslib <path-to-library>/libXrdTokenAuthzOfs.so
ofs.trace all debug
